            <Font name="System Bold" size="22.0" />
         </font>
         <padding>
            <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
         </padding>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label layoutX="600.0" layoutY="64.0" text="Finished Appointments">
         <font>
            <Font name="System Bold" size="29.0" />
         </font>
      </Label>
      <TableView fx:id="finishview" layoutX="355.0" layoutY="131.0" prefHeight="470.0" prefWidth="802.0">
        <columns>
          <TableColumn fx:id="AppDaycolumnfinish" prefWidth="75.0" text="Appointments Day" />
          <TableColumn fx:id="AppDatecolumnfinish" prefWidth="75.0" text="Appointments Date" />
            <TableColumn fx:id="AppTimecolumnfinish" prefWidth="75.0" text="Appointments Time" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
