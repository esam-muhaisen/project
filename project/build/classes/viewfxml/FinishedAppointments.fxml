<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="650.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.FinishedAppointmentsController">
   <children>
      <AnchorPane prefHeight="663.0" prefWidth="303.0" style="-fx-background-color: #2980b9;">
         <children>
            <Button fx:id="waitingAppfinfshed" layoutX="5.0" layoutY="330.0" mnemonicParsing="false" onAction="#setonwaitingAppfinfshed" prefHeight="52.0" prefWidth="309.0" style="-fx-background-color: #2980b9;" text="Waiting Appointments" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="25.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </children>
      </AnchorPane>
      <Label layoutX="98.0" layoutY="51.0" prefHeight="73.0" prefWidth="149.0" text="Hello" textFill="WHITE">
         <font>
            <Font name="System Bold" size="49.0" />
         </font>
      </Label>
      <Label layoutX="21.0" layoutY="118.0" text="Our patient" textFill="WHITE">
         <font>
            <Font name="System Bold" size="50.0" />
         </font>
      </Label>
      <Button fx:id="freeAppfinfshed" layoutX="3.0" layoutY="244.0" mnemonicParsing="false" onAction="#setonfreeAppfinfshed" prefHeight="51.0" prefWidth="302.0" style="-fx-background-color: #2980b9;" text="Free Appointments" textFill="WHITE">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="finishtap" layoutX="-6.0" layoutY="417.0" mnemonicParsing="false" onAction="#setonfinishtap" prefHeight="52.0" prefWidth="320.0" style="-fx-background-color: black;" text="Finished Appointments" textFill="WHITE">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="logoutfinished" layoutX="90.0" layoutY="574.0" mnemonicParsing="false" onAction="#setonlogoutfinished" style="-fx-background-color: black;" text="&lt;-log out" textFill="WHITE">
