<AnchorPane id="AnchorPane" prefHeight="650.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.PatientLoginController">
   <children>
      <Button fx:id="swithch" layoutX="24.0" layoutY="14.0" mnemonicParsing="false" onAction="#switchtomain" prefHeight="39.0" prefWidth="98.0" style="-fx-background-color: black;" text="&lt;-Go Back" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="lognid" layoutX="756.0" layoutY="357.0" mnemonicParsing="false" onAction="#onlogin" prefHeight="43.0" prefWidth="111.0" style="-fx-background-color: #1e3799;" text="login" textFill="WHITE">
         <padding>
            <Insets bottom="10.0" left="25.0" right="25.0" top="10.0" />
         </padding>
         <font>
            <Font size="27.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label layoutX="522.0" layoutY="278.0" text="Password:">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label layoutX="506.0" layoutY="177.0" text="User Name:">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label layoutX="540.0" layoutY="472.0" text="are you new here?" textFill="#0c43f5">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <Button fx:id="registerid" layoutX="749.0" layoutY="460.0" mnemonicParsing="false" onAction="#actionregister" prefHeight="55.0" prefWidth="134.0" style="-fx-background-color: blue;" text="Register " textFill="WHITE">
         <padding>
            <Insets bottom="10.0" left="15.0" right="15.0" top="10.0" />
         </padding>
         <font>
            <Font size="21.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <PasswordField fx:id="Passwordid" layoutX="711.0" layoutY="276.0" onAction="#onPasswordid" prefHeight="54.0" prefWidth="255.0" promptText="Password">
